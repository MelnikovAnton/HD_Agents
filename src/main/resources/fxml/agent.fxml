<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="ru.melnikov.hdAgent.controllers.AgentController">
    <children>
        <AnchorPane maxHeight="50.0" minHeight="50.0" prefHeight="50.0">
            <children>
                <HBox alignment="CENTER_LEFT" nodeOrientation="LEFT_TO_RIGHT" prefHeight="50.0" prefWidth="268.0"
                      AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                      AnchorPane.topAnchor="0.0">
                    <children>
                        <Label fx:id="eventAgentLable" alignment="CENTER" contentDisplay="CENTER" HBox.hgrow="ALWAYS">
                            <HBox.margin>
                                <Insets left="10.0"/>
                            </HBox.margin>
                        </Label>
                        <AnchorPane prefHeight="25.0" prefWidth="66.0" HBox.hgrow="ALWAYS">
                            <HBox.margin>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="15.0"/>
                            </HBox.margin>
                            <children>
                                <ProgressBar fx:id="agentTableprogress" progress="0.0" AnchorPane.leftAnchor="10.0"
                                             AnchorPane.rightAnchor="10.0"/>
                            </children>
                        </AnchorPane>
                    </children>
                </HBox>
            </children>
        </AnchorPane>
        <AnchorPane>
            <children>
                <TableView fx:id="agentsTable" editable="true" AnchorPane.bottomAnchor="10.0"
                           AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
                    <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                    </columnResizePolicy>
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </padding>
                </TableView>
            </children>
        </AnchorPane>
    </children>
</VBox>
